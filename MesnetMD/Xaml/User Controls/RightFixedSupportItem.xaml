<UserControl x:Class="MesnetMD.Xaml.User_Controls.RightFixedSupportItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" >
    
    <StackPanel Orientation="Horizontal">
        <Grid Height="20" Width="20">
            <Polygon Points="7.5,2 7.5,16 8.5,16 8.5,2" Fill="Black"/>
            <Polygon Points="8.5,2 12.5,5 12.5,6 8.5,3" Fill="Black"/>
            <Polygon Points="8.5,5 12.5,8 12.5,9 8.5,6" Fill="Black"/>
            <Polygon Points="8.5,8 12.5,11 12.5,12 8.5,9" Fill="Black"/>
            <Polygon Points="8.5,11 12.5,14 12.5,15 8.5,12" Fill="Black"/>
            <Polygon Points="8.5,14 12.5,17 12.5,18 8.5,15" Fill="Black"/>
        </Grid>

        <TextBlock Margin="5,0,0,0" x:Name="supportheader" VerticalAlignment="Center" ></TextBlock>
    </StackPanel>
</UserControl>
