<UserControl x:Class="MesnetMD.Xaml.User_Controls.ConcentratedLoadItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MesnetMD.Xaml.User_Controls"
             mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <Grid Height="20" Width="20">
            <Polygon Points="9,2 11,2 11,14 13,14 10,18 7,14 9,14" Fill="Black"></Polygon>
        </Grid>

        <TextBlock Margin="5,0,0,0" x:Name="concload" VerticalAlignment="Center" ></TextBlock>
    </StackPanel>
</UserControl>
