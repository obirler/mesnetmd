                    <TextBlock Text="MPa" VerticalAlignment="Center"/>
                </StackPanel>

                <Border Height="1"  BorderThickness="0,1,0,0" BorderBrush="#FFC3C2C2" Background="#FFE1E1E1" Margin="0,5,0,5"/>

                <Expander x:Name="uiexpand" Margin="5,5,0,5" Header="{DynamicResource uniforminertia}" Width="300" IsExpanded="true" Expanded="uiexpand_Expanded" >

