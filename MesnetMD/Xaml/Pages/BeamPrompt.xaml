                                    <TextBox x:Name="dli" Text="0" IsEnabled="True" Width="50" Height="21" Margin="2,2,0,0"></TextBox>
                                    <TextBlock Text="cm" FontSize="18" Margin="5,5,0,0"></TextBlock>
                                </StackPanel>

                            </StackPanel>

                        </StackPanel>

