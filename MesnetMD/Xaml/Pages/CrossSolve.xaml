<Window x:Class="MesnetMD.Xaml.Pages.CrossSolve"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="{DynamicResource crosssolver}" Height="150" Width="300" Icon="/Assets/solve.ico" WindowStartupLocation="CenterOwner">
    <StackPanel>
        <TextBlock x:Name="status" Height="30" Width="272" TextAlignment="Center" TextWrapping="WrapWithOverflow" Margin="10,20,10,10" />
        <ProgressBar x:Name="progress" IsIndeterminate="False" Height="25" Margin="10,10,10,10" Maximum="100" Minimum="0" ></ProgressBar>
    </StackPanel>
</Window>