<UserControl x:Class="MesnetMD.Xaml.User_Controls.InertiaItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MesnetMD.Xaml.User_Controls"
             mc:Ignorable="d">
    
    <StackPanel Orientation="Horizontal">
        <Grid Height="20" Width="20">
            <Polygon Points="4,4 16,4 16,6 4,6" Fill="Black"/>
            <Polygon Points="4,16 16,16 16,14 4,14" Fill="Black"/>
            <Polygon Points="9,6 9,14 11,14 11,6" Fill="Black"></Polygon>

        </Grid>

        <TextBlock Margin="5,0,0,0" x:Name="inertiatext" VerticalAlignment="Center" ></TextBlock>
    </StackPanel>
</UserControl>
